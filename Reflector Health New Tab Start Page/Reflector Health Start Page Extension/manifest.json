{
  "name": "Dangerboard Start Page",
  "description" : "When a new tab is created, this extension will load a page with Google search and Apple Rings",
  "version": "2.0",
  "manifest_version": 3,
	"chrome_url_overrides" : {
		"newtab": "index.html"
	},
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.dangerboard.com/*",
    "http://localhost/*"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
    }
}
